{
  "files.watcherExclude": {
    "**/routeTree.gen.ts": true
  },
  "search.exclude": {
    "**/routeTree.gen.ts": true
  },
  "files.readonlyInclude": {
    "**/routeTree.gen.ts": true
  },
  "editor.wordBasedSuggestions": "off",
  "editor.quickSuggestionsDelay": 200,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.defaultFormatter": "dprint.dprint",
  "editor.formatOnSave": true,
  "rust-analyzer.check.command": "check",
  "rust-analyzer.check.workspace": false,
  "rust-analyzer.checkOnSave": true,
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.cargo.extraEnv": {
    "MACOSX_DEPLOYMENT_TARGET": "14.2"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#b3002d",
    "activityBar.background": "#b3002d",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#289f00",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#b3002d",
    "statusBar.background": "#800020",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#b3002d",
    "statusBarItem.remoteBackground": "#800020",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#800020",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#80002099",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#800020"
}
